{
  "name": "@testmachine.ai/cli",
  "version": "0.1.63",
  "description": "The command-line interface for TestMachine.",
  "type": "module",
  "main": "build/index.js",
  "bin": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testmachine-ai/cli.git"
  },
  "author": "Oliver Hill",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/testmachine-ai/cli/issues"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^14.11.2",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.2",
    "tsx": "^3.12.6",
    "typescript": "~4.7.0"
  },
  "scripts": {
    "build": "tsc",
    "execute": "tsx ./src/index.ts",
    "repo:create": "tsx ./src/index.ts --token $TOKEN repo create",
    "snapshot:create": "tsx ./src/index.ts --token $TOKEN snapshot create",
    "repo:list": "tsx ./src/index.ts --token $TOKEN repo list",
    "snapshot:list": "tsx ./src/index.ts --token $TOKEN snapshot list --repo-id=$REPO_ID",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.3.4",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "dedent": "^0.7.0",
    "form-data": "^4.0.0",
    "hyperid": "^3.1.1",
    "inquirer": "^9.2.10",
    "ora": "^6.1.2",
    "tmp": "^0.2.1",
    "zod": "^3.22.1",
    "tty-table": "^4.2.1"
  },
  "homepage": "https://github.com/testmachine-ai/cli#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "blockchain",
    "security"
  ]
}
